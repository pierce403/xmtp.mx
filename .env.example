## Frontend
# Get from thirdweb dashboard: https://thirdweb.com/create-api-key
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=

# Optional: used for GitHub Pages project sites (e.g. "/xmtp.mx")
NEXT_PUBLIC_BASE_PATH=

# Optional: improves ENS resolution reliability in the browser
NEXT_PUBLIC_MAINNET_RPC_URL=

# Optional: XMTP environment for the browser client (local|dev|production). Defaults to production.
NEXT_PUBLIC_XMTP_ENV=production

## SMTP -> XMTP bridge (not deployed on GitHub Pages)
# Wallet used by the bridge to publish XMTP messages (keep this secret)
XMTP_BOT_PRIVATE_KEY=

# Optional: XMTP environment (local|dev|production). Defaults to production.
XMTP_ENV=production

# Optional: improves ENS resolution reliability on the server
ETHEREUM_RPC_URL=
